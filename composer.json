{
    "name": "chemaclass/phel-cli-gui",
    "keywords": [
        "phel",
        "lisp",
        "functional",
        "language"
    ],
    "homepage": "https://phel-lang.org/",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-readline": "*",
        "phel-lang/phel-lang": "dev-master",
        "symfony/console": "^5.1 || ^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "5.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "PhelCliGui\\": "src/php/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "8.0.24"
        }
    }
}
